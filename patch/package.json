{
	"name": "patch-package-to-pnpm",
	"version": "0.1.0",
	"description": "Transform legacy code patterns",
	"type": "module",
	"devDependencies": {
		"@codemod.com/jssg-types": "^1.0.9",
		"@types/node": "20.9.0",
		"typescript": "^5.8.3"
	},
	"scripts": {
